{
  "name": "datasmyth",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Carbon Digital",
    "email": "hello@carbondigital.us",
    "url": "https://carbondigital.us/"
  },
  "main": "src/index.js",
  "bin": {
    "datasmyth": "./bin/run"
  },
  "devDependencies": {
    "@oclif/test": "2.1.1",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "oclif": "^3.2.0"
  },
  "dependencies": {
    "@dbml/core": "^2.4.4",
    "@oclif/core": "1.12.1",
    "@oclif/plugin-help": "5.1.12",
    "axios": "0.27.2",
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "inquirer": "^7.0.1",
    "netrc-parser": "^3.1.6",
    "open": "^7.0.0",
    "ora": "^4.0.3",
    "update-notifier": "^4.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "oclif": {
    "additionalHelpFlags": ["help"],
    "additionalVersionFlags": ["version"],
    "commands": "./src/commands",
    "bin": "dbdocs",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./src/hooks/init/checkUpdate",
        "./src/hooks/init/loadEnv"
      ],
      "prerun": [
        "./src/hooks/prerun/checkVersion"
      ]
    }
  },
  "repository": "carbondigitalus/datasmyth",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif manifest",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif readme && git add README.md"
  }
}
